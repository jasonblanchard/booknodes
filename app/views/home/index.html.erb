<div class='user-actions'>
  <% if user_signed_in? %>
    <%= link_to 'Sign Out', destroy_user_session_path, :method => :delete %>
  <% end %>
</div>

<% if @events %>
  <%= div_for @events do |event| %>
    <%= link_to event.title, event  %>
  <% end %>
<% end %>
<br />
<br />
<%= link_to "Sign in with Goodreads", user_omniauth_authorize_path(:goodreads) %>
